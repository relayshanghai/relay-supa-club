{
  "name": "relay-supa-club",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:staged": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=-1",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:sql": "sqlfluff lint --ignore=parsing --disable-progress-bar supabase/seed.sql supabase/policies supabase/functions/ supabase/tests",
    "fix:sql": "sqlfluff fix --ignore=parsing --disable-progress-bar --show-lint-violations supabase/seed.sql supabase/policies supabase/functions/ supabase/tests",
    "type-check:app": "tsc",
    "type-check-all": "run-p type-check:*",
    "test": "jest",
    "tailwind:build": "tailwindcss -i ./styles/globals.css -o ./build/index.css",
    "tailwind:watch": "tailwindcss -i ./styles/globals.css -o ./build/index.css --watch",
    "test-cypress-unit": "run-s tailwind:build test:run-cypress-unit",
    "test:watch": "jest --watch",
    "test:run-cypress-unit": "cypress run --component",
    "cypress": "npx cypress open",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@sentry/nextjs": "^7.48.0",
    "@supabase/auth-helpers-nextjs": "^0.6.1",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.20.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "fuse.js": "^6.6.2",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "idb": "^7.1.1",
    "next": "^13.3.0",
    "openai": "^3.2.1",
    "preline": "^1.8.0",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.2.0",
    "react-select": "^5.7.2",
    "react-visibility-sensor": "^5.1.1",
    "recharts": "^2.5.0",
    "rudder-sdk-js": "^2.30.0",
    "sib-api-v3-sdk": "^8.5.0",
    "stripe": "^12.1.1",
    "swr": "^2.1.3",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "@testing-library/cypress": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.10.0",
    "eslint": "8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "supabase": "^1.50.8",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.1.0",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "(supabase|supabase/policies|supabase/functions|supabase/tests)/**/(*|seed).sql": [
      "command -v sqlfluff &>/dev/null",
      "sqlfluff fix --force --disable-progress-bar --ignore=parsing"
    ],
    "supabase/tests/**/*.sql": [
      "supabase/db.sh db_test"
    ],
    "(pages|src|styles|types|i18n|docs)/**/!(*supabase).ts": [
      "prettier --no-error-on-unmatched-pattern --write"
    ],
    "(pages|src|styles|types|i18n|docs)/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext=.js,.jsx,.ts,.tsx --max-warnings=0 --fix"
    ]
  },
  "engines": {
    "node": "18.14.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "volta": {
    "node": "18.14.0",
    "npm": "9.3.1"
  }
}
