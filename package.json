{
  "name": "relay-supa-club",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:staged": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=44",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:sql": "sqlfluff lint --ignore=parsing --disable-progress-bar supabase/seed.sql supabase/policies supabase/functions/ supabase/tests",
    "fix:sql": "sqlfluff fix --ignore=parsing --disable-progress-bar --show-lint-violations supabase/seed.sql supabase/policies supabase/functions/ supabase/tests",
    "type-check:app": "tsc -p tsconfig.build.json",
    "type-check-all": "run-p type-check:*",
    "test": "vitest run",
    "tailwind:build": "tailwindcss -i ./styles/globals.css -o ./build/index.css",
    "tailwind:watch": "tailwindcss -i ./styles/globals.css -o ./build/index.css --watch",
    "test-cypress-unit": "run-s tailwind:build test:run-cypress-unit",
    "test:watch": "vitest",
    "test:run-cypress-unit": "cypress run --component",
    "cypress": "npx cypress open",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "supabase": "npx supabase start",
    "start-email-server": "cd email-server && docker-compose up -d"
  },
  "dependencies": {
    "@analytics/storage-utils": "^0.4.2",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@rudderstack/rudder-sdk-node": "^2.0.2",
    "@sentry/nextjs": "^7.48.0",
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^1.54.0",
    "@supabase/auth-helpers-nextjs": "^0.6.1",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.20.0",
    "@tanstack/react-table": "^8.9.3",
    "analytics": "^0.8.9",
    "bottleneck": "^2.19.5",
    "callsites": "^4.0.0",
    "clsx": "^2.0.0",
    "cookies-next": "^2.1.2",
    "d3-selection": "^3.0.0",
    "date-fns": "^2.29.3",
    "dompurify": "^3.0.5",
    "dotenv": "^16.0.3",
    "fuse.js": "^6.6.2",
    "googleapis": "^118.0.0",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "idb": "^7.1.1",
    "jotai": "^2.0.4",
    "nanoid": "^4.0.2",
    "next": "^13.3.0",
    "node-object-hash": "^3.0.0",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-d3-cloud": "^1.0.6",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.2.0",
    "react-select": "^5.7.2",
    "react-visibility-sensor": "^5.1.1",
    "recharts": "^2.5.0",
    "rudder-sdk-js": "^2.30.0",
    "sib-api-v3-sdk": "^8.5.0",
    "stripe": "^12.1.1",
    "swr": "^2.1.3",
    "tailwind-merge": "^1.14.0",
    "ulid": "^2.3.0",
    "use-analytics": "^1.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-mdx-gfm": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/nextjs": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/cypress": "^9.0.0",
    "@types/dompurify": "^3.0.2",
    "@types/node": "18.15.11",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/use-analytics": "^0.0.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.13.0",
    "cypress-iframe": "^1.0.1",
    "eslint": "8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "storybook": "^7.0.7",
    "supabase": "^1.83.7",
    "tailwindcss": "^3.3.1",
    "typescript": "5.0.4",
    "vitest": "^0.30.1"
  },
  "lint-staged": {
    "(supabase|supabase/policies|supabase/functions|supabase/tests)/**/(*|seed).sql": [
      "sqlfluff fix --force --disable-progress-bar --ignore=parsing"
    ],
    "supabase/tests/**/*.sql": [
      "supabase/db.sh db_test"
    ],
    "(pages|src|styles|types|i18n|docs|cypress)/**/!(*supabase).{ts,tsx}": [
      "prettier --no-error-on-unmatched-pattern --write --editorconfig"
    ],
    "(pages|src|styles|types|i18n|docs|cypress)/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext=.js,.jsx,.ts,.tsx --max-warnings=0 --fix"
    ]
  },
  "engines": {
    "node": "18.14.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "volta": {
    "node": "18.14.0",
    "npm": "9.3.1"
  }
}
