  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/cypress": "^9.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.7.0",
    "eslint": "8.34.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "msw": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "supabase": "^1.42.4",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "(pages|src|styles|types|i18n|docs)/**/!(*supabase).ts": [
      "prettier --no-error-on-unmatched-pattern --write"
    ],
    "(pages|src|styles|types|i18n|docs)/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext=.js,.jsx,.ts,.tsx --max-warnings=0 --fix"
    ]
  },
  "engines": {
    "node": "18.14.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "volta": {
    "node": "18.14.0",
    "npm": "9.3.1"
  }
