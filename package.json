    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.8.0",
    "@types/lodash": "^4.14.193",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "fuse.js": "^6.6.2",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "idb": "^7.1.1",
    "lodash": "^4.17.21",
    "next": "^13.1.6",
    "openai": "^3.1.0",
    "preline": "^1.7.0",
    "react": "^18.2.0",
    "react-countup": "^6.4.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.1.5",
    "react-select": "^5.7.0",
    "react-visibility-sensor": "^5.1.1",
    "recharts": "^2.4.3",
    "rudder-sdk-js": "^2.28.0",
    "sib-api-v3-sdk": "^8.5.0",
    "stripe": "^11.11.0",
    "swr": "^2.1.2",
    "ulid": "^2.3.0"
