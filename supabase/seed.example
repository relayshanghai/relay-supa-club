  ) VALUES (
    '00000000-0000-0000-0000-000000000000',
    user_id,
    'authenticated',
    'authenticated',
    email,
    crypt('password123!',
      gen_salt('bf')),
    '2023-02-18 23:31:13.017218+00',
    NULL,
    '',
    '2023-02-18 23:31:12.757017+00',
    '',
    NULL,
    '',
    '',
    NULL,
